{
  "manifest_version": 2,
  "name": "CURVE chrome extension",
  "version": "1.0",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "chrome.png",
    "48": "chrome.png",
    "128": "chrome.png"
  }
}
